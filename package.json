{
  "name": "hydra-web-service",
  "version": "0.1.0",
  "description": "Web service for information from Hydra backend.",
  "repository": {
    "type": "git",
    "url": "https://github.com/usgs/hydra-web-service.git"
  },
  "keywords": [
    "usgs"
  ],
  "scripts": {
    "install": "node src/lib/pre-install --non-interactive",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "node_modules/.bin/eslint 'src/**/*.js'",
    "lint:test": "node_modules/.bin/eslint 'test/**/*.js'",
    "start": "node src/server.js",
    "test": "node_modules/.bin/mocha 'test/**/*Test.js' --reporter spec",
    "uninstall": "node src/lib/uninstall --non-interactive",
    "watch:src": "watch 'npm run lint:src && npm run test' src",
    "watch:test": "watch 'npm run lint:test && npm run test' test"
  },
  "license": "CC0-1.0",
  "dependencies": {
    "express": "^4.14.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.1.1",
    "extend": "^3.0.0",
    "mocha": "^2.5.3",
    "pre-commit": "^1.1.3",
    "sinon": "^1.17.4",
    "watch": "^0.19.1"
  },
  "engines": {
    "node": ">=4.2.4"
  }
}
